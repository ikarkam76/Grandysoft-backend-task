<html>
    <body>
        <h1><%-title %></h1>
        <table class="table table-striped table-primary">
        <thead>
          <tr>
            <th>Number</th>
            <th>Name</th>
            <th>Gender</th>
            <th>Descriptions</th>
          </tr>
        </thead>
        <tbody>
          <%if (records.length >0){ %>
          <% records.forEach(function(row){ %>
          <tr>
            <td><%= row.number %></td>
            <td><%= row.first_name %></td>
            <td><%= row.gender %></td>
            <td><%= row.desc %></td>
          </tr>
          <% })}else{ %>
            <tr>
                <td colspan="6">No Record Found</td>
              </tr>
            <% } %>
        </tbody>
      </table>
    </body>
</html>